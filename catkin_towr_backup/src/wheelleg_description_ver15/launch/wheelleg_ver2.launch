<launch>
  
  <!-- Upload URDF file to ros parameter server for rviz to find   wheelleg_description_ver15.urdf    -->
  <param name="wheelleg_rviz_urdf_robot_description" 
   command="$(find xacro)/xacro '$(find wheelleg_description_ver15)/urdf/wheelleg_total_ver2.urdf.xacro'"/>
   
  
  <!-- Converts Cartesian state to joint state and publish TFs to rviz  --> 
  <node name="urdf_visualizer_wheelleg" pkg="xpp_hyq" type="urdf_visualizer_wheelleg" output="screen"/>
  <!-- <node name="urdf_visualizer_hyq4" pkg="xpp_hyq" type="urdf_visualizer_hyq4" output="screen"/> -->

</launch>