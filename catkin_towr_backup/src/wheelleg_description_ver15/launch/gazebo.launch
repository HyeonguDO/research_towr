<launch>
  <include
    file="$(find gazebo_ros)/launch/empty_world.launch" />

  <!-- Convert xacro and put on parameter server -->
  <param name="robot_description" command="$(find xacro)/xacro '$(find wheelleg_description_ver15)/urdf/wheelleg_description_ver15.urdf'"/>

  <node name="urdf_spawner" 
        pkg="gazebo_ros" 
        type="spawn_model" 
        respawn="false" 
        output="screen" 
        args="-urdf -model wheelleg_total_ver2 -param robot_description -z 0.8"/>

  <!-- <node
    name="fake_joint_calibration"
    pkg="rostopic"
    type="rostopic"
    args="pub /calibrated std_msgs/Bool true" /> -->


</launch>